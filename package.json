{
  "name": "OnePlusLife",
  "author": "Tor Harrington",
  "version": "1.7.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
    "displayName": "OnePlus Life",
    "uuid": "e71435f5-87fe-4e67-9ec3-d78b5cf8349c",
    "sdkVersion": "3",
    "enableMultiJS": true,
    "targetPlatforms": [
      "aplite",
      "basalt",
      "chalk"
    ],
    "capabilities": [
      "configurable"
    ],
    "watchapp": {
      "watchface": true
    },
    "messageKeys": [
      "THEME",
      "IS_SAYINGS",
      "IS_NOTCH"
    ],
    "resources": {
      "media": [
        {
          "type": "font",
          "name": "LATO_BLACK_24",
          "file": "fonts/Lato-Black.ttf",
          "characterRegex": "[0-9:A-Za-z !#?]"
        },
        {
          "type": "font",
          "name": "ROKKITT_BOLD_64",
          "file": "fonts/Rokkitt-Bold.ttf",
          "characterRegex": "[0-9]"
        },
        {
          "menuIcon": true,
          "type": "bitmap",
          "name": "IMAGE_MENU_ICON",
          "file": "menu_icon.png"
        },
        {
          "type": "bitmap",
          "name": "IMAGE_NOTCH",
          "file": "notch.png"
        }
      ]
    }
  }
}
